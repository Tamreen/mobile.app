<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.tamreen.tamreen" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>تمرين</name>
  <description>
        لتنظيم الحضور للتمارين.
    </description>
  <author email="hossam_zee@yahoo.com" href="http://hossamzee.github.io/">
      حسام الزغيبي
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-navigation href="*"/>
  <allow-intent href="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <icon src="resources/android/icon/drawable-ldpi.png" density="ldpi"/>
    <icon src="resources/android/icon/drawable-mdpi.png" density="mdpi"/>
    <icon src="resources/android/icon/drawable-hdpi.png" density="hdpi"/>
    <icon src="resources/android/icon/drawable-xhdpi.png" density="xhdpi"/>
    <icon src="resources/android/icon/drawable-xxhdpi.png" density="xxhdpi"/>
    <icon src="resources/android/icon/drawable-xxxhdpi.png" density="xxxhdpi"/>
    <splash src="resources/android/splash/land-hdpi.png" density="land-hdpi"/>
    <splash src="resources/android/splash/land-ldpi.png" density="land-ldpi"/>
    <splash src="resources/android/splash/land-mdpi.png" density="land-mdpi"/>
    <splash src="resources/android/splash/land-xhdpi.png" density="land-xhdpi"/>
    <splash src="resources/android/splash/land-xxhdpi.png" density="land-xxhdpi"/>
    <splash src="resources/android/splash/land-xxxhdpi.png" density="land-xxxhdpi"/>
    <splash src="resources/android/splash/port-hdpi.png" density="port-hdpi"/>
    <splash src="resources/android/splash/port-ldpi.png" density="port-ldpi"/>
    <splash src="resources/android/splash/port-mdpi.png" density="port-mdpi"/>
    <splash src="resources/android/splash/port-xhdpi.png" density="port-xhdpi"/>
    <splash src="resources/android/splash/port-xxhdpi.png" density="port-xxhdpi"/>
    <splash src="resources/android/splash/port-xxxhdpi.png" density="port-xxxhdpi"/>
  </platform>
  <platform name="ios">
    <icon src="resources/ios/icon/appicon-60.png" width="60" height="60"/>
    <icon src="resources/ios/icon/appicon-60@2x.png" width="120" height="120"/>
    <icon src="resources/ios/icon/appicon-60@3x.png" width="180" height="180"/>
    <icon src="resources/ios/icon/appicon-72.png" width="72" height="72"/>
    <icon src="resources/ios/icon/appicon-72@2x.png" width="144" height="144"/>
    <icon src="resources/ios/icon/appicon-76.png" width="76" height="76"/>
    <icon src="resources/ios/icon/appicon-76@2x.png" width="152" height="152"/>
    <icon src="resources/ios/icon/appicon-small-40.png" width="40" height="40"/>
    <icon src="resources/ios/icon/appicon-small-40@2x.png" width="80" height="80"/>
    <icon src="resources/ios/icon/appicon-small-50.png" width="50" height="50"/>
    <icon src="resources/ios/icon/appicon-small-50@2x.png" width="100" height="100"/>
    <icon src="resources/ios/icon/appicon-small.png" width="29" height="29"/>
    <icon src="resources/ios/icon/appicon-small@2x.png" width="58" height="58"/>
    <icon src="resources/ios/icon/appicon-small@3x.png" width="87" height="87"/>
    <icon src="resources/ios/icon/appicon.png" width="57" height="57"/>
    <icon src="resources/ios/icon/appicon@2x.png" width="114" height="114"/>
    <splash src="resources/ios/splash/default-568h@2x.png" width="640" height="1136"/>
